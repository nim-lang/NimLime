[
    {
        "id": "preferences",
        "children":
        [
            {
                "id": "package-settings",
                "children":
                [
                    {
                        "caption": "Nim",
                        "children":
                        [
                            {
                                "command": "open_url",
                                "args": {
                                    "url": "https://github.com/nim-lang/NimLime/issues"
                                },
                                "caption": "Support/Issue Tracker"
                            },

                            { "caption": "-" },

                            {
                                "command": "edit_settings", "args":
                                {
                                    "base_file": "${packages}/Nim/Preferences.sublime-settings",
                                    "default": "{\n\t$0\n}\n"
                                },
                                "caption": "Settings"
                            },

                            {
                                "command": "edit_settings", "args":
                                {
                                    "base_file": "${packages}/Nim/Default.sublime-keymap",
                                    "default": "[\n\t$0\n]\n"
                                },
                                "caption": "Key Bindings"
                            }
                        ]
                    }
                ]
            }
        ]
    }
]
